{
  "name": "src-run/cocoa-var-dumper-library",
  "license": "MIT",
  "type": "library",
  "homepage": "https://src.run/cocoa-var-dumper-library",
  "description": "A simple variable var-dumper/transformer library wrapping the symfony/var-dumper component.",
  "support": {
    "email": "rmf@src.run",
    "issues": "https://src.run/cocoa-var-dumper-library/issues",
    "source": "https://src.run/cocoa-var-dumper-library/git"
  },
  "keywords": [ "cocoa-var-dumper-library", "cocoa", "var-dumper", "library", "rmf", "src-run" ],
  "authors": [
    {
      "name": "Rob Frawley 2nd",
      "email": "rmf@src.run",
      "homepage": "https://src.run/rmf",
      "role": "Project Lead"
    }
  ],
  "require": {
    "php": "^8.1",
    "src-run/augustus-utility-library": "~0.10",
    "symfony/var-dumper": "~4.1 | ~5.0 | ~6.0"
  },
  "require-dev": {
    "codacy/coverage": "~1.4",
    "friendsofphp/php-cs-fixer": "~3.2",
    "php-coveralls/php-coveralls": "~2.1",
    "symfony/phpunit-bridge": "~4.1 | ~5.0 | ~6.0"
  },
  "autoload": {
    "psr-4": {
      "SR\\Dumper\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SR\\Dumper\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.20-dev"
    }
  }
}
